{
  "name": "@karmats/fundamentals",
  "version": "1.0.0",
  "description": "Fundamental UI components such as colors and icons",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.cjs",
  "files": [
    "dist",
    "./colors.d.ts",
    "./icons.d.ts"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./index.d.ts"
    },
    "./colors": {
      "import": "./dist/colors.js",
      "require": "./dist/colors.cjs",
      "types": "./colors.d.ts"
    },
    "./icons": {
      "import": "./dist/icons.js",
      "require": "./dist/icons.cjs",
      "types": "./icons.d.ts"
    }
  },
  "author": "Mats Roshauw",
  "license": "MIT",
  "scripts": {
    "build": "vite build",
    "build:types": "tsc"
  },
  "devDependencies": {
    "@mdi/js": "^7.1.96",
    "tsc": "^2.0.4",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-dts": "^2.1.0"
  }
}
