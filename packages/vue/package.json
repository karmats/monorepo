{
  "type": "module",
  "files": ["dist"],
  "main": "./dist/karmats-vue.umd.cjs",
  "module": "./dist/karmats-vue.js",
  "exports": {
    ".": {
      "import": "./dist/karmats-vue.js",
      "require": "./dist/karmats-vue.umd.cjs"
    }
  },
  "name": "@karmats/vue",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run build:types",
    "build:types": "vue-tsc --project tsconfig.build-types.json --declaration --emitDeclarationOnly --outDir dist/types ",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@types/node": "^18.14.6",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/tsconfig": "^0.1.3",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vue-tsc": "^1.0.24"
  }
}
