{
  "name": "@karmats/vue-lib",
  "version": "1.0.0",
  "private": true,
  "files": [
    "dist"
  ],
  "types": "dist/types",
  "type": "module",
  "main": "./dist/karmats-vue-lib.umd.cjs",
  "module": "./dist/karmats-vue-lib.js",
  "exports": {
    ".": {
      "import": "./dist/karmats-vue-lib.js",
      "require": "./dist/karmats-vue-lib.umd.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "build": "yarn clean && vue-tsc && vite build && yarn build:types && yarn changeset publish",
    "build:types": "vue-tsc --project tsconfig.types.json",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "vue": "^3.2.45",
    "@karmats/fundamentals": "*"
  },
  "devDependencies": {
    "@babel/types": "^7.21.2",
    "@types/node": "^18.14.6",
    "@vitejs/plugin-vue": "^4.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vue-tsc": "^1.0.24"
  }
}
